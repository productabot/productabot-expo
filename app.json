{
  "expo": {
    "name": "productabot",
    "slug": "productabot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "productabot",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.productabot",
      "usesIcloudStorage": true,
      "buildNumber": "2022.04.12.1",
      "associatedDomains": [
        "applinks:app.productabot.com"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "name": "productabot",
      "favicon": "./assets/images/favicon.png",
      "backgroundColor": "#000000"
    },
    "description": ""
  }
}